\">\n              <option v-for=\"line in lineList\" v-bind:key=\"line.filter_type\" v-on:click=\"getLineFilters\">{{line.filter_type}} </option>\n            </select>\n          </th>\n          <th>{{Lselected}}</th>\n        </tr>\n        <tr>\n          <th>Select spesific filter</th>\n          <th>\n            <select v-model=\"Fselected\">\n              <option v-for=\"filter in filtersList\" v-bind:key=\"filter.filter_name\"> {{filter.filter_name}} </option>\n            </select>\n          </th>\n          <th>{{Fselected}}</th>\n        </tr>\n      </table>\n    </div>\n    <br>\n    <button v-on:click=\"getFilterSensorLimits(); getFilterDataBetweenDates();\">Generate</button>\n    <div>\n      <table v-for=\"limits in limitsList\" v-bind:key=\"limits.id_filter_type\">\n        <tr>\n          <th>Line name</th>\n          <th>Filter sensor name</th>\n          <th>Upper limit</th>\n          <th>Lower limit</th>\n        </tr>\n        <tr>\n          <th>{{Lselected}}</th>\n          <th>{{Fselected}}</th>\n          <th>{{limits.weight_greater_then}}</th>\n          <th>{{limits.weight_less_then}}</th>\n        </tr>\n      </table>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nimport DataPicker from 'vue2-datepicker';\nimport 'vue2-datepicker/index.css';\nimport moment from 'moment';\nimport axios from 'axios';\n\nexport default {\n  name: 'FrontEnd',\n  components: {\n    DataPicker\n  },\n  data() {\n    return {\n      startDate: '',\n      endDate: '',\n      Lselected: '',\n      Fselected: '',\n      lineList: [],\n      filtersList: [],\n      limitsList: [],\n      filterDataList: [],\n    }\n  },\n  props: {\n    msg: String\n  },\n  methods: {\n\n     getLineList() {\n      const path = \"https://smartsystemsroject.herokuapp.com/api/filterTypes\";\n      axios.get(path)\n        .then(response => {\n          this.lineList = response.data.data;\n          console.log(this.lineList);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n\n     getLineFilters() {\n       console.log(this.Lselected);\n      if(this.Lselected != ''){\n        let path = \"https://smartsystemsroject.herokuapp.com/api/\"+this.Lselected+\"/filters\";\n        axios.get(path)\n          .then(response => {\n            this.filtersList = response.data.data;\n            console.log(path);\n            console.log(this.Lselected);\n            //console.log(this.filtersList);\n          })\n          .catch((error) => {\n            console.error(error);\n          });\n      }\n    },\n\n    getFilterSensorLimits() {\n      console.log(this.Fselected);\n      if(this.Fselected != ''){\n        let path = \"http://localhost:2000/api/\"+this.Lselected+\"/\"+this.Fselected+\"\";\n        axios.get(path)\n          .then(response => {\n            this.limitsList = response.data.data;\n            console.log(this.limitsList);\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n\n    getFilterDataBetweenDates() {\n      if(this.startDate != '' || this.endDate != ''){\n        this.startDate = moment(this.startDate).format('YYYY-MM-DD');\n        this.endDate = moment(this.endDate).format('YYYY-MM-DD');\n        let path= \"http://localhost:2000/api/\"+this.Lselected+\"/\"+this.Fselected+\"/\"+this.startDate+\"&\"+this.endDate;\n        axios.get(path)\n          .then(response => {\n            this.filterDataList = response.data.data;\n            console.log(this.filterDataList);\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n\n  },\n  created() {\n      this.getLineList();\n      this.getLineFilters();\n      this.getFilterSensorLimits();\n      this.getFilterDataBetweenDates();\n  }\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh1 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n#dataP {\n  margin: 400px 0 0;\n}\n</style>\n"]}]}